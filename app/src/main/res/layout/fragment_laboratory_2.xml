<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="详细信息"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp" />

    <View
        android:id="@+id/v_1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/line"
        android:layout_marginHorizontal="10dp"
        app:layout_constraintTop_toBottomOf="@id/tv_2" />

    <TextView
        android:id="@+id/tv_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="名称："
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/v_1"
        app:layout_constraintStart_toStartOf="@id/v_1"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="信息系统与理论实验室"
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/v_1"
        app:layout_constraintStart_toEndOf="@id/tv_3"
        android:layout_marginStart="50dp"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="房间号："
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/tv_3"
        app:layout_constraintStart_toStartOf="@id/tv_3"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="南区实训楼301-303"
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/tv_4"
        app:layout_constraintStart_toStartOf="@id/tv_4"
        android:layout_marginTop="10dp" />


    <TextView
        android:id="@+id/tv_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="负责人："
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/tv_5"
        app:layout_constraintStart_toStartOf="@id/tv_5"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/tv_6"
        app:layout_constraintStart_toStartOf="@id/tv_6"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="简介："
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/tv_9"
        app:layout_constraintStart_toStartOf="@id/tv_9"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="无"
        style="@style/TextStyle_black_14sp"
        android:gravity="start|center"
        app:layout_constraintTop_toBottomOf="@id/tv_10"
        app:layout_constraintStart_toStartOf="@id/tv_10"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="占用课程："
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/tv_11"
        app:layout_constraintStart_toStartOf="@id/tv_11"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="无"
        style="@style/TextStyle_black_14sp"
        android:gravity="start|center"
        app:layout_constraintTop_toBottomOf="@id/tv_12"
        app:layout_constraintStart_toStartOf="@id/tv_12"
        android:layout_marginTop="10dp" />


    <TextView
        android:id="@+id/tv_17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="学生设备数："
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/tv_13"
        app:layout_constraintStart_toStartOf="@id/tv_13"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="70"
        style="@style/TextStyle_black_14sp"
        android:gravity="start|center"
        app:layout_constraintTop_toBottomOf="@id/tv_14"
        app:layout_constraintStart_toStartOf="@id/tv_14"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="最新公告："
        style="@style/TextStyle_black_14sp"
        app:layout_constraintTop_toBottomOf="@id/tv_17"
        app:layout_constraintStart_toStartOf="@id/tv_17"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_20"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="无"
        style="@style/TextStyle_black_14sp"
        android:gravity="start|center"
        app:layout_constraintTop_toBottomOf="@id/tv_18"
        app:layout_constraintStart_toStartOf="@id/tv_18"
        app:layout_constraintEnd_toEndOf="@id/v_1"
        android:layout_marginTop="10dp" />

    <TextView
        android:id="@+id/tv_21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="设备报修>>>"
        android:textSize="14sp"
        android:textColor="@color/red"
        android:gravity="start|center"
        app:layout_constraintTop_toBottomOf="@id/tv_20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="10dp" />


</androidx.constraintlayout.widget.ConstraintLayout>